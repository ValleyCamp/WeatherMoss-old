			"title": "Current Wind (Every 15 sec over 3 hr)",
			"width": 1,
			"row": {
				"2": 1,
				"3": 1,
				"4": 1
			},
			"col": {
				"2": 1,
				"3": 1,
				"4": 1
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "text_widget",
					"settings": {
						"size": "regular",
						"value": "datasources[\"ws15sec\"][\"payload\"][\"WindSpeedCur\"]",
						"sparkline": true,
						"animate": true,
						"units": "MPH"
					}
				},
				{
					"type": "pointer",
					"settings": {
						"direction": "datasources[\"ws15sec\"][\"payload\"][\"WindDirCur\"]",
						"value_text": "datasources[\"ws15sec\"][\"payload\"][\"WindDirCurEng\"]"
					}
				}
			]
		},
